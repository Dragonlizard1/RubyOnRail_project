<h1>Create New User</h1>
<% if flash[:notice] %>
  <% flash[:notice].each do |note| %>
    <p id="notice"><%= note %></p>  
  <% end %>
<% end %>
<form action = "/users/new" method = "post">
  <input type = 'hidden' name = "authenticity_token" value = "<%=form_authenticity_token%>">
  <label>Name</label>
  <input type = "text" name = "name" id = "user_first_name">
 
  <input type = "submit" value = "Create User" id = "Create User1">
</form>